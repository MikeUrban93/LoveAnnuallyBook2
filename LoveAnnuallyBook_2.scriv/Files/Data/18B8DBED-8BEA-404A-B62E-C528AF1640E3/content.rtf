\par\plain {\f2\fs22\b1\i0\cf7 - Time:}{\f3\fs22\b0\i0\cf1 <$Scr_Cs::5> Morning<!$Scr_Cs::5>}
\par\plain {\f2\fs22\b1\i0\cf7 - Weather/Environment:}{\f3\fs22\b0\i0\cf1 <$Scr_Cs::5> Overcast, storms ahead.<!$Scr_Cs::5><!$Scr_Ps::4>}
\par\pard\plain \tx0\fi360\ltrch\loch \f0\fs18\b0\i0
\par\pard\plain \tx0\ltrch\loch {\f2\fs22\b1\i0\cf5 <$Scr_Ps::2>Characters Present:<!$Scr_Ps::2><$Scr_Ps::4>}
\par\pard\plain \tx0\li346\fi14\sb60\sl264\slmult1\ltrch\loch {\f2\fs22\b1\i0\cf7 - Main Characters:}{\f3\fs22\b0\i0\cf1 <$Scr_Cs::5> Evan, Sarah, Liz<!$Scr_Cs::5>}
\par\plain {\f2\fs22\b1\i0\cf7 - Secondary Characters:}{\f3\fs22\b0\i0\cf1 <$Scr_Cs::5> Evan's moderately priced divorce attorney and Sarah's high-priced senior attorney and a junior attorney.<!$Scr_Cs::5><!$Scr_Ps::4>}
\par\pard\plain \tx0\fi360\ltrch\loch \f0\fs18\b0\i0
